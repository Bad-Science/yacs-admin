<%=form_for @school do |c|%>
  <div class="form-group">
  <%= c.label :id %>
  <%= c.text_field :id, :class => "form-control"%>
</div>

  <div class="form-group">
  <%= c.label :name %>
  <%= c.text_field :name, :class => "form-control"%>
</div>

  <%= submit_tag "Submit", :class => "btn btn-search" %>

<%end%>